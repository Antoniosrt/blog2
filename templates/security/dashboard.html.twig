{% extends 'base.html.twig' %}

{% block body %}
    <br>
    <div class="col-12">
        <h1>Dashboard Usuario </h1>
    <ul class="list-group">
        {% for user in users %}
            <li class="list-group-item">
                <a href="{{ path('user.edit', {id: user.id}) }}">
                    {{ user.username }}
                </a>
            </li>
        {% endfor %}
    </ul>
    </div>
{% endblock %}